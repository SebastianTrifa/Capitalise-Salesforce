    @isTest
    static void testReadProfile()
    {
        Opportunity oppRecord = [select Id, CapitaliseExtId__c, Name, Funding_Specialist__c from Opportunity where Name Like 'test search prev#%' limit 1];
        Set<String> testopps = new Set<String>();
        testopps.add(oppRecord.CapitaliseExtId__c);

        Test.startTest();
        Test.setMock(HttpCalloutMock.class, new CapAPPMock_Fail());
        Map<String, Object> result = CapAPPCallouts.GetSearchProfileById(oppRecord.Id);
        Test.setMock(HttpCalloutMock.class, new CapAPPMock_SemiFail());
        result = CapAPPCallouts.GetSearchProfileById(oppRecord.Id);
        result = CapAPPCallouts.GetSearchProfileById(oppRecord.Funding_Specialist__c);
        Test.setMock(HttpCalloutMock.class, new CapAPPMock_Success());
        result = CapAPPCallouts.GetSearchProfileById(oppRecord.Id);
        CapAPPCallouts.UpdatePaidOutFundingSearches(testopps);
        Test.stopTest();

        System.Assert((Boolean)result.get('success'));
    }
